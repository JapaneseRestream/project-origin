		"react-admin": "^4.16.18",
		"remix-auth": "^3.7.0",
		"remix-auth-discord": "^1.3.3",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.7.3",
		"@cloudflare/workers-types": "^4.20240512.0",
